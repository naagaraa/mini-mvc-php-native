{
    "name": "nagara/mini-mvc-php-native",
    "description": "MINI MVC PHP NATIVE PROJECT",
    "homepage": "https://github.com/naagaraa/mini-mvc-phpnative",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "framework",
        "mini mvc php"
    ],
    "authors": [
        {
            "name": "nagara",
            "email": "ekabersinar@gmail.com",
            "role": "developer"
        },
        {
            "name": "sandhika galih",
            "role": "dosen"
        }
    ],
    "autoload": {
        "psr-4": {
            "MiniMvc\\": "apps/core"
        }
    },
    "require": {
        "php": ">=7.3.2",
        "phpoffice/phpspreadsheet": "^1.14*@dev",
        "twbs/bootstrap": "4.5.2*@dev",
        "mjaschen/phpgeo": "^3.1*@dev",
        "mpdf/mpdf": "^8.0*@dev",
        "vlucas/phpdotenv": "^5.2*@dev",
        "bramus/router": "~1.5*@dev"
    },
    "require-dev": {
        "php": ">=7.3.2",
        "phpoffice/phpspreadsheet": "^1.14*@dev",
        "twbs/bootstrap": "4.5.2*@dev",
        "mjaschen/phpgeo": "^3.1*@dev",
        "mpdf/mpdf": "^8.0*@dev",
        "vlucas/phpdotenv": "^5.2*@dev",
        "bramus/router": "~1.5*@dev"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/naagaraa/mini-mvc-phpnative.git"
        }
    ]
}