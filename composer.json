{
    "name": "nagara/mini-mvc-php-native",
    "description": "MINI MVC PHP NATIVE PROJECT",
    "homepage": "https://github.com/naagaraa/mini-mvc-php-native",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "framework",
        "native",
        "php",
        "oop",
        "mini mvc php native"
    ],
    "authors": [
        {
            "name": "nagara",
            "email": "ekabersinar@gmail.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "bramus/router": "~1.5",
        "markrogoyski/math-php": "^1.10",
        "mpdf/mpdf": "^8.0",
        "phpmailer/phpmailer": "^6.2",
        "phpoffice/phpspreadsheet": "^1.14",
        "robmorgan/phinx": "^0.12.6",
        "rubix/ml": "^0.4.1",
        "rubix/tensor": "^2.2",
        "symfony/console": "^5.2",
        "twig/twig": "3.0",
        "vlucas/phpdotenv": "^5.2"
    },
    "require-dev": {
        "php": ">=7.3.2",
        "bramus/router": "~1.5",
        "phpmailer/phpmailer": "^6.2",
        "symfony/var-dumper": "^5.2",
        "twig/twig": "3.0",
        "vlucas/phpdotenv": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "app\\": "apps",
            "Console\\": "system",
            "Rubix\\":"vendor/rubix/ml/src/"
        },
        "classmap": [
            "apps",
            "system",
            "vendor/rubix/ml/src/"
        ]
    },
    "archive": {
        "directory": "dist",
        "skip-dev": false
      },
      "scripts": {
        "post-create-project-cmd": [
            "@php nagara generate:env"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/naagaraa/mini-mvc-php-native.git"
        }
    ]
}
